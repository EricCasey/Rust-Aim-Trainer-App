    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.7.0",
    "react-canvas": "^1.3.0",
    "react-cursor-position": "^3.0.3",
    "react-dom": "^16.7.0",
    "react-interval": "^2.1.1",
    "react-keyboard-event-handler": "^1.5.4",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
