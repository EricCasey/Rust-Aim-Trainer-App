{
  "name": "lookup-client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "devDependencies": {
    "enzyme": "3.8.0",
    "react-addons-test-utils": "15.6.2",
    "react-scripts": "2.1.3"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "6.24.1",
    "get-image-pixels": "^1.0.1",
